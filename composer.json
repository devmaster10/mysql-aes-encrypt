{
    "name": "codiant/dbaesencrypt",
    "type": "library",
    "description": "Laravel MySql AES Encrypt/Decrypt",
    "keywords": ["laravel","mysql", "aes", "encrypt"],
    "homepage": "https://github.com/Seldaek/monolog",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Mahendra Gurjar",
            "email": "mahendra.gurjar@codiant.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/config": ">=5.4",
        "illuminate/database": ">=5.4",
        "illuminate/support": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "DevMaster10\\AESEncrypt\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
            
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
